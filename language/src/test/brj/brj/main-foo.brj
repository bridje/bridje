(ns brj.main-foo
  {:aliases {b brj.main-bar
             Foo (java brj.FooKt
                       (:: (isZero Int) Bool)
                       (:: (dec Int) Int)
                       (:: (conj #{a} a) #{a})
                       (:: (plus Int Int) Int)
                       (:: (str Str Str) Str))}
   :refers {brj.main-bar #{baz}}})

(:: :Void)
(:: Void (+ :Void))

(:: (:Just a) a)
(:: :Nothing)

(:: :first-name Str)
(:: :last-name Str)
;; (:: User {:first-name :last-name})

(#_ (def (count-down x)
  (loop [y x
         res #{}]
    (if (Foo/isZero y) res (recur (Foo/dec y) (Foo/conj res y))))))

(#_ (def (vector el) [el]))

(:: :user {})
(#_ (def my-user {:user {:first-name "James", :last-name "Henderson"}}))

(def (say-hi m)
  (str "Hi, " (:first-name (:user m))))

