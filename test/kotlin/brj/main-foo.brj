(ns foo
  {:aliases {b bar
             Foo (java brj.FooKt)}
   :refers {bar #{baz}}})

(:: (Foo/isZero Int) Bool)
(:: (Foo/dec Int) Int)
(:: (Foo/conj #{a} a) #{a})
(:: (Foo/plus Int Int) Int)
(:: (Foo/str Str Str) Str)
(:: (Foo/println Str) Str)

(:: :Void)
(:: Void (+ :Void))

(:: (:Just a) a)
(:: :Nothing)

(:: (! (println! Str)) Str)
(def (! (println! s))
  (Foo/println s))

(:: :first-name Str)
(:: :last-name Str)
;; (:: User {:first-name :last-name})

(def (count-down x)
  (loop [y x
         res #{}]
    (if (Foo/isZero y) res (recur (Foo/dec y) (Foo/conj res y)))))

(:: (my-fn a a) [a])
(def (my-fn x y)
  (with-fx [(def (println! s)
              (foo/println! (Foo/str "intercepted: " s)))]
    (println! "Hello world!")
    [y x]))

(def (vector el) [el])

(def foo
  (-> []
      :forms/VectorForm
      vector
      :forms/ListForm))
