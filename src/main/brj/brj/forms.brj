(ns brj.forms)

(:: Form (+ :BooleanForm :StringForm
            :IntForm :BigIntForm :FloatForm :BigFloatForm
            :ListForm :SetForm :VectorForm :RecordForm
            :SymbolForm :QSymbolForm))

(:: :BooleanForm Bool)
(:: :StringForm Str)

(:: :IntForm Int)
(:: :BigIntForm BigInt)
(:: :FloatForm Float)
(:: :BigFloatForm BigFloat)

(:: :ListForm [Form])
(:: :SetForm [Form])
(:: :VectorForm [Form])
(:: :RecordForm [Form])

(:: :SymbolForm Symbol)
(:: :QSymbolForm QSymbol)
